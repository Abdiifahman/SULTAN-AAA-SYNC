import os
import sys

class QadrLicense:
    def __init__(self):
        self.owner = "SULTAN-AAA"
        self.royalty_clause = "Commercial use requires a royalty fee."
        
    def check_license(self):
        # ุงูุชุญูู ููุง ุฅุฐุง ูุงู ุงููุณุชุฎุฏู ูุฏ ุฏูุน ุฃู ูููู ุชุตุฑูุญุงู
        if not os.path.exists(".qadr_license_key"):
            print(f"--- [ Qadr Engine Alert ] ---")
            print(f"Owner: {self.owner}")
            print(f"Notice: {self.royalty_clause}")
            print(f"Contact SULTAN-AAA for commercial licensing.")
            
            # ุงูุณูุงุญ ุจุงูุงุณุชุฎุฏุงู ุงูุดุฎุตู ููุท ุฃู ุงูุฅุบูุงู
            usage_type = input("Is this commercial use? (y/n): ")
            if usage_type.lower() == 'y':
                print("Access Denied. Please pay the specified royalty.")
                sys.exit()

# ุชุดุบูู ุงููุญุต ุนูุฏ ุจุฏุงูุฉ ุชุดุบูู ุงููุญุฑู
license_system = QadrLicense()
license_system.check_license()



