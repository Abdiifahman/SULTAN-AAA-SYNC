#!/usr/bin/env python3
# decode_base64.py
# أداة بسيطة لفك سلاسل Base64 من سطر الأوامر أو من ملف
import sys, base64, argparse, os

def try_decode(s):
    s = s.strip()
    try:
        b = base64.b64decode(s, validate=True)
        try:
            return b.decode('utf-8')
        except UnicodeDecodeError:
            return repr(b)
    except Exception as e:
        return f"[ERROR] not valid base64: {e}"

def main():
    parser = argparse.ArgumentParser(description="Decode Base64 strings (single or file).")
    parser.add_argument("input", nargs="?", help="Base64 string or path to file containing strings (one per line).")
    parser.add_argument("-o", "--output", help="Write decoded results to OUTPUT file (JSON lines).")
    args = parser.parse_args()

    outputs = []
    if args.input is None:
        # read from stdin
        for line in sys.stdin:
            line = line.strip()
            if not line: 
                continue
            decoded = try_decode(line)
            outputs.append({"input": line, "decoded": decoded})
    elif os.path.isfile(args.input):
        with open(args.input, "r", encoding="utf-8", errors="ignore") as f:
            for line in f:
                line = line.strip()
       
