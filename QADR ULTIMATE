            return {
                'success': True,
                'plugin': plugin_name,
                'result': result,
                'timestamp': time.time()
            }
        except Exception as e:
            return {
                'success': False,
                'plugin': plugin_name,
                'error': str(e),
                'timestamp': time.time()
            }
    
    def list_plugins(self, module_type: QADRModuleType = None) -> List[QADRPlugin]:
        """Ø¹Ø±Ø¶ Ø§Ù„Ø¥Ø¶Ø§ÙØ§Øª Ø§Ù„Ù…ØªØ§Ø­Ø©"""
