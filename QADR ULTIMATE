        return decorator
    
    async def execute_plugin(self, plugin_name: str, *args, **kwargs) -> Any:
        """ØªÙ†ÙÙŠØ° Ø¥Ø¶Ø§ÙØ© Ù…Ø­Ø¯Ø¯Ø©"""
